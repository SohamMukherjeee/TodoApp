{
    "version": 2,
    "builds": [
      {
        "src": "app.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/app.js"
      }
    ],
    "functions": {
      "api/**/*.js": {
        "maxDuration": 10,
        "regions": ["sfo1", "cdg1"]
      }
    },
    "env": {
      "MONGODB_URL": "@mongodb_url",
      "PORT": "3000"
    }
  }
  